{
  "name": "reclaimed-fe",
  "version": "0.20.182",
  "description": "Reclaimed App Front End",
  "keywords": [
    "candee",
    "generations",
    "camp",
    "software",
    "react"
  ],
  "author": {
    "name": "Tyler Candee",
    "email": "tyler@candeegenerations.com",
    "url": "https://candeegenerations.com"
  },
  "license": "MIT",
  "scripts": {
    "start": "npm rebuild node-sass && nodemon -w craco.config.js -w ./src/content/antd.less --exec 'craco start'",
    "build": "rimraf build && REACT_APP_VERSION=$npm_package_version craco build",
    "test": "craco test",
    "test:ci": "CI=true yarn test"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "jest": {
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "transformIgnorePatterns": [
      "/node_modules/(?!antd)/.+\\.js$"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm version patch && git add package.json && lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "@craco/craco": "^5.6.4",
    "@emotion/core": "^10.0.28",
    "antd": "^3.26.14",
    "axios": "^0.19.2",
    "craco-antd": "^1.14.1",
    "deep-equal": "^2.0.1",
    "deepmerge": "^4.2.2",
    "jwt-decode": "^2.2.0",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "node-sass": "^4.13.1",
    "prettier": "^2.0.2",
    "prop-types": "^15.7.2",
    "qs": "^6.9.3",
    "react": "^16.12.0",
    "react-app-rewired": "^2.1.5",
    "react-dnd": "^10.0.2",
    "react-dnd-html5-backend": "^10.0.2",
    "react-dom": "^16.12.0",
    "react-router5": "^7.1.0",
    "react-scripts": "3.2.0",
    "rimraf": "^3.0.2",
    "router5": "^7.0.2",
    "router5-helpers": "^7.0.1",
    "router5-plugin-browser": "^7.0.2",
    "router5-plugin-logger": "^7.0.2",
    "simple-crypto-js": "^2.2.0",
    "use-deep-compare-effect": "^1.3.1"
  },
  "devDependencies": {
    "@babel/plugin-proposal-optional-chaining": "^7.9.0",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@testing-library/jest-dom": "^5.3.0",
    "@testing-library/react": "^10.0.1",
    "babel-jest": "24.9.0",
    "babel-plugin-emotion": "^10.0.29",
    "babel-plugin-import": "^1.12.2",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "customize-cra": "^0.9.1",
    "eslint-config-kentcdodds": "CandeeGenerations/eslint-config-kentcdodds",
    "eslint-config-prettier": "^6.10.1",
    "eslint-config-react": "^1.1.7",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-babel-module": "^5.1.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "husky": "^4.2.3",
    "less": "^3.11.1",
    "less-loader": "^5.0.0",
    "lint-staged": "^10.0.9",
    "nodemon": "^2.0.2"
  }
}
